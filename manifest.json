{
    "manifest_version": 2,

    "name": "Zillow Console",
    "description": "This extends the console with colors and stack traces.",
    "version": "1.0",

    "icons": {
        "128": "zillow-console-128.png"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_start",
            "js": ["Embed.js"]
        }
    ],

    "web_accessible_resources": [
        "Logging.js"
    ]
}
